@article{Lovasz78,
 abstract="Considers the problem of finding the maximum rank of the
     matching of a polymatroid. Polymatroid is like a matroid
     but the rank of each element can be greater than 1, e.g.,
     edges in a graph each have rank 2, but the rank of a cycle
     is 2n-1 where n is the number of edges in the cycle.
     Matching is a set of elements whose rank is the sum of the
     element ranks thus the maximum matching in a graph is the
     largest possible set of edges containing no cycles. This
     paper shows that the maximum matching problem is usually
     NP, but also demonstrates a polynomial-time algorithm when
     the polymatroid can be projected to two dimensions e.g. a
     hypergraph or graph",
 address="Szeged, Hungary",
 author="L. Lov{\'{a}}sz",
 journal="Colloquia Mathematica Societatis Janos Bolyai",
 pages="495-517",
 title="The Matroid Matching Problem",
 year="1978"
}

