@article{Hickok07,
 abstract="lexical access from acoustics involves two pathways: a
     bilateral ventral pathway and a left-dominant dorsal
     pathway. Both pathways depend on spectrotemporal processing
     in the dorsal superior temporal gyrus and phonological
     processing including effects of phonological lexical
     neighborhood size in the mid-posterior superior temporal
     sulcus. Ventral pathway then accesses a
     phonological-to-conceptual mapping in the posterior medial
     temporal gyrus and posterior inferior temporal sulcus with
     final access to a conceptual network widely distributed in
     the cortex. There is some evidence that phonological
     processing in right hemisphere is specialized to longer
     time scales e.g. syllabic time scales while segmental time
     scales may be left specialized or may be bilateral. The
     anterior temporal lobe e.g. anterior medial temporal gyrus
     and anterior inferior temporal sulcus may be involved in
     lexical access but authors suggest a role in syntactic and
     combinatorial semantic processing. Dorsal pathway may
     involve a general purpose sensorimotor interface in
     parietal temporal sylvian fissure followed by access to the
     articulatory network in posterior inferior frontal gyrus,
     primary motor areas, and anterior insula. There is evidence
     that lexical access can be achieved by either the left or
     right HS ventral pathway or by the dorsal pathway
     independently and is thus robust to many types of
     single-hemisphere lesion",
 author="Gregory Hickok and David Poeppel",
 journal="Nature Reviews",
 keywords="neuroanatomy, fMRI, speech perception, human speech
     recognition, lexical access",
 pages="393-402",
 publisher="Nature Publishing Group",
 title="The cortical organization of speech processing",
 volume="8",
 year="2007"
}

