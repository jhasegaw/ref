@article{Smallwood73, 
 author="Richard D. Smallwood and Edward J. Sondik",
 title="The Optimal Control of Partially Observable Markov Processes
   over a Finite Horizon",
 pages="1071--1088",
 abstract="POMDP defined to be an HMM whose transition and 
   observation probability matrices depend on the control strategy.
   vector of posterior state probabilities is shown to be a sufficient
   statistic, which evolves according to a continuous-state discrete-time
   process given by the state update T(posterior|observation,control).
   Transformation is linear followed by projection back onto the simplex,
   therefore the total reward resulting from any particular control
   strategy is a linear function of the starting vector pi.  Optimum
   control strategy is the max of these, thus for a finite horizon of
   n time steps and K strategies it is a piece-wise linear convex surface
   with up to O{K^n} facets",
  volume="21",
  number="5",
  journal="Operations Research",
  year="1973"
}

